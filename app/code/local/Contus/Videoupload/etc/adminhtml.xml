<config>
    <menu>
         <all translate="title" module="all">
            <children>
                <videoupload translate="title">
                    <title>Manage Deal Videos</title>
                    <sort_order>1</sort_order>
                    <action>videoupload/adminhtml_videoupload</action>
                </videoupload>
            </children>
        </all>
    </menu>
    <acl>
        <resources>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <videoupload translate="title" module="videoupload">
                                        <title>Video Upload</title>
                                    </videoupload>
                                </children>
                            </config>
                        </children>
                    </system>
                    <catalog translate="title" module="catalog">
                        <children>
                             <videoupload translate="title" module="videoupload">
                                <title>Manage Videoupload</title>
                            </videoupload>
                        </children>
                    </catalog>
                </children>
            </admin>
        </resources>
    </acl>
</config>
